    rdi 0
    wr sp
    rd io
    call fib
    wr io
    sig brk

fib:isp -3
    strs 0 # argument
    rd dl
    strs 1
    rd dh
    strs 2

    rdi 0
    wr x
    rdi 257$0 # = 1
    wr y
fib_loop:
    lds 0
    subi 1
    jif carry fib_done
    strs 0
    rd x
    add y
    sw y
    wr x
    j fib_loop
fib_done:
    rd y
    strs 0
    lds 1
    wr dl
    lds 2
    wr dh
    lds 0
    isp 3
    ja